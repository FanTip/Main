
<div ng-model="hid"></div>
<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <div class="embed-responsive embed-responsive-16by9">
                <iframe class="embed-responsive-item" src="https://player.vimeo.com/video/148051964" width="200%" height="600" frameborder="0" ></iframe>
            </div>
        </div>
    </div>

    {{# if login}}
       
    {{else}}
        <div class="row">
            <div class="col-lg-12">
                <h3 class="text-center">Give and receive cashless tips with </h3>
                <h3 class="text-center">FanTipper.</h3>
                <h4 class="text-center"><a href="">Android</a> and <a href="">iOS</a> app now available.</h4>
            </div>
            <div class="col-lg-12 text-center">
                <button class="btn btn-primary">Find out more</button><h5> OR </h5><button class="btn btn-primary">Get Involved</button>
            </div>
        </div>
    {{/if}}

    <div class="row">
        <div class="col-mg-12">
            Description
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class="" id="search">
                <div class="bg-grey p-4">
                    <form class="form-inline my-2 my-lg-12">
                        <input class="form-control mr-sm-1" type="search" placeholder="Search">
                        <button class="btn btn-outline-success my-2 my-sm-12" type="submit">Search</button>
                    </form>
                </div>
            </div>
        </div>
    </div>
    
    <div class="row">
            {{#each objects}}
                <div class="card" style="width: 18rem;">
                    {{!-- <img class="card-img-top" src="{{this.imagepath}}" alt="image"> --}}
                    <div class="card-body">
                        {{!-- <h5 class="card-title">Footpath Thoughts</h5>
                        <p class="card-text">Spreading good vibes to all you beautiful humans of the world.</p> --}}
                        <p id = "users">{{this.creator}}</p>
                        <button class="btn btn-primary" onclick="takeid()">Fantip</button>
                        <script>
                            function takeid(){
                                var creator = document.getElementById('users').innerHTML;
                                alert(creator);
                            }
                        </script>
                    </div>
                </div>
            {{/each}}
    </div>
    

</div>


